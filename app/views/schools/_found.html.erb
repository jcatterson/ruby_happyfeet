<table>
  <thead>
    <tr>
      <th>School Name</th>
      <th>Coachs Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @found.each do |school| %>
      <tr>
        <td><%=school.name%></td>
        <td>
          <% if school.coach != nil%>
            <%=school.coach.full_name%>
          <%end%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
