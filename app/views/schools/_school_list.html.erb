<table>
  <tr>
    <th>School</th>
    <th>Coach</th>
  </tr>
  <% if @found %>
    <% @found.each do |x| %>
      <tr>
        <td><%=x.name%></td>
        <td>
          <% if x.coach != nil %>
            <%= x.coach.full_name %>
          <%end%>
        </td>
      </tr>
    <%end%>
  <%end%>
</table>
