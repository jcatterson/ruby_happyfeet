<h1>Listing student_attendances</h1>

<table>
  <thead>
    <tr>
      <th>Attendance</th>
      <th>Student</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @student_attendances.each do |student_attendance| %>
      <tr>
        <td><%= student_attendance.attendance_id %></td>
        <td><%= student_attendance.student_id %></td>
        <td><%= link_to 'Show', student_attendance %></td>
        <td><%= link_to 'Edit', edit_student_attendance_path(student_attendance) %></td>
        <td><%= link_to 'Destroy', student_attendance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student attendance', new_student_attendance_path %>
